    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}\\app.js",
            "env": {
                "ACCOUNT_SID" : "{twilio account sid}",
                "AUTH_TOKEN" : "{twilio auth token}",
                "AUTH" : "Basic {endpoint auth}",
                "ENV" : "development",
                "REPLY_NUMBER" : ""
            }
        }
    ]
}